{
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "files.associations": {
    "*.rs": "rust"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "rust-analyzer.check.command": "clippy",
  "css.validate": false,
  "files.exclude": {
    "**/.yarn/": false,
    "**/.cargo/": false,
    "**/.vscode/": false,
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    ".clippy.toml": false,
    ".deny.toml": false,
    ".gitattributes": false,
    ".gitignore": false,
    ".pnp.cjs": false,
    ".pnp.loader.mjs": false,
    ".taplo.toml": false,
    ".yarnrc.yml": false,
    "Cargo.lock": false,
    "rust-toolchain.toml": false,
    "yarn.lock": false
  }
}